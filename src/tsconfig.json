{"dependencies": {
  "react": "^17.0.2",
  "react-dom": "^17.0.2",
  "react-router-dom": "^6.2.1",
  "proxy": "http://localhost:3001/"
},
  "compilerOptions": {
    "lib": ["dom","esnext","es2015", "es6", "dom.iterable", "scripthost"

    ],
    "strict": false,
    "esModuleInterlop": true,
    "allowJs": true,
    "skipLibCheck": true,
